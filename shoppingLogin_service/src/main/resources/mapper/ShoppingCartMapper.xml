<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.cart.module.mapper.UserMapper">


    <select id="findByPhoneAndPwd" resultType="java.util.HashMap">
        select * from loginuser where loginName =#{loginName} and userPassword =#{userPassword}
    </select>


</mapper>